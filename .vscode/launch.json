{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8000",
        "--timeout-keep-alive",
        "600"
      ],
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/app"
      },
      "console": "integratedTerminal"
    }
  ]
}